{
	"expo": {
		"name": "pulse-app",
		"slug": "pulse-app",
		"owner": "max.kang",
		"privacy": "unlisted",
		"version": "1.0.0",
		"runtimeVersion": "1.0.0",
		"platforms": ["ios", "android", "web"],
		"orientation": "portrait",
		"icon": "./assets/images/logo-lg.png",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./assets/images/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#F7F7F7"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"buildNumber": "1",
			"supportsTablet": true
		},
		"android": {
			"buildNumber": 1,
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/logo-sm.png",
				"backgroundColor": "#ffffff"
			}
		},
		"web": {
			"favicon": "./assets/images/logo-sm.png"
		},
		"extra": {
			"eas": {
				"projectId": "18adc0d0-eb1d-11e9-8009-d524ed5cc4a7"
			}
		},
		"updates": {
			"url": "https://u.expo.dev/18adc0d0-eb1d-11e9-8009-d524ed5cc4a7"
		},

		"plugins": [
			"expo-asset",
			[
				"expo-font",
				{
					"fonts": ["./assets/fonts/SUIT-Regular.ttf"]
				}
			],
			[
				"expo-image-picker",
				{
					"photosPermission": "The app accesses your photos to let you share them with your friends."
				}
			]
		]
	}
}
